{
    "manifest_version": 3,
    "name": "Pomo",
    "version": "1.0.0",
    "description": "An extension intended to help with productivity that revolves around the Pomodora study method.",
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      },
    "action": {
        "default_icon": {
            "16": "icons/icon16.png",
            "32": "icons/icon32.png",
            "48": "icons/icon48.png",
            "128": "icons/icon128.png"
        },
        "default_title": "Lock In",
        "default_popup": "popup.html"
    },
    "permissions": [
        "storage",
        "notifications"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "options_page": "options.html"
  }